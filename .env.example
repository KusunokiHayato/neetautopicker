# Google Apps Script設定（必須）
# GASでWeb Appをデプロイした後、そのURLをここに設定してください
VITE_GAS_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# アプリケーション設定
VITE_APP_NAME=LoL Team Composition Automation System
VITE_APP_VERSION=1.0.0

# 環境設定
VITE_NODE_ENV=production

# デバッグ・ログ設定
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=error

# API設定
VITE_API_TIMEOUT=30000
VITE_REQUEST_RETRY_COUNT=3

# セキュリティ設定（本番環境推奨）
VITE_ALLOWED_ORIGINS=https://your-domain.com

# Google Sheets設定（オプション）
# VITE_SHEET_ID=your-google-sheet-id
# VITE_SHEET_NAME=players

# 機能フラグ
VITE_ENABLE_DISCORD_EXPORT=true
VITE_ENABLE_REROLL_SYSTEM=true
VITE_ENABLE_SUB_ACCOUNT_FILTER=true
VITE_ENABLE_TIME_FILTER=true
